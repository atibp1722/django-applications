#:kivy 2.1.0

<MainWidget>:
    Screen:

        NavigationLayout:
            ScreenManager:
                id:screen_manager
                
                HomePage:

                    name:'home page'
                    BoxLayout:
                        orientation:'vertical'
                        MDToolBar:
                            title:'Weather App'
                            elevation:10
                            left_action_items:[['menu',lambda x:nav_drawer.toggle_nav_drawer()]]
                        ScrollView:
            
            MDNavigationDrawer:
                id:nav_drawer
                ContentNavigationDrawer:
                    id:content_drawer
                    screen_manager:screen_manager
                    nav_drawer:nav_drawer
